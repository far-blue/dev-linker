{
	"name": "far-blue/dev-linker",
	"type": "composer-plugin",
	"description": "A composer plugin to link local working copies of packages",
	"require": {
		"composer-plugin-api": "1.*"
	},
	"require-dev": {
		"composer/composer": "1.*"
	},
	"minimum-stability": "dev",
	"autoload": {
		"psr-0": { "DevLinker": "src/" }
	},
	"extra": {
		"class": "DevLinker\\Plugin"
	}
}
